{
  "name": "react-resume",
  "version": "0.1.0",
  "author": {
    "name": "Timo Aho",
    "email": "timo.aho@pm.me"
  },
  "description": "React resume component",
  "repository": "git@github.com:Zeukkari/react-resume.git",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build:storybook": "cross-env NODE_ENV=production build-storybook -c .storybook -s build -o dist/storybook",
    "check": "npm run lint && npm test",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest src",
    "test:coverage": "jest src --coverage && codecov"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "grommet": "^2.3.1",
    "grommet-controls": "^1.0.10",
    "grommet-icons": "^4.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "resume-schema": "github:Zeukkari/resume-schema#zeukkari/update-deps",
    "styled-components": "^4.1.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@storybook/react": "^4.1.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-grommet": "^0.5.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chart.js": "^2.7.2",
    "codecov": "^3.1.0",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-parallel": "^0.3.2",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "pre-commit": "^1.2.2",
    "react-dev-utils": "^5.0.0",
    "react-imported-component": "^5.4.1",
    "react-chartjs-2": "^2.7.4",
    "react-test-renderer": "^16.7.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/templates/",
      "<rootDir>/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|templates|node_modules)[/\\\\]"
    ]
  },
  "pre-commit": [
    "check"
  ]
}